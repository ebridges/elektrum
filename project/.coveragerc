[run]
source = .
branch = True
omit=
    base/*
    elektrum/*
    status/*
    manage.py
    */admin.py
    */apps.py
    */meta/*
    */migrations/*
    */models.py
    */urls.py
    */__init__.py
    */fields.py
    */forms.py
    */scripts/*
    */tests*
    conftest.py

[report]
show_missing = True
exclude_lines =
  pragma: no cover
  def __unicode__
  def __repr__
  def __str__
  if self\.debug
  class Meta
  from
  import


  # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
